{
  "name": "judge",
  "description": "imba server and client template, with live server and client reloading, environment separation",
  "keywords": [
    "imba",
    "boilerplate"
  ],
  "author": "dipsywong98",
  "contributors": [
    "mcreng"
  ],
  "bugs": "https://github.com/dipsywong98/imba-template/issues",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "npm run build && cross-env NODE_ENV=production node src/server/index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server-dev \" \"npm run watch\"",
    "server-dev": "nodemon --watch src/ -e js,imba,ejs --exec cross-env NODE_ENV=dev node src/server/index.js",
    "build": "webpack",
    "watch": "webpack --watch --progress --colors --display-modules --env.NODE_ENV=dev"
  },
  "files": [],
  "directories": {},
  "homepage": "http://imba.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/somebee/hello-world-imba.git"
  },
  "license": "MIT",
  "dependencies": {
    "chokidar": "^2.0.4",
    "connect-mongodb-session": "^2.0.6",
    "ejs": "^2.6.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "imba": "^1.4.0",
    "imba-router": "^0.2.0",
    "mongodb": "^3.1.10",
    "passport": "^0.4.0",
    "passport-openidconnect": "0.0.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^3.1.11"
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.17.4",
    "webpack-cli": "^3.1.2"
  }
}
